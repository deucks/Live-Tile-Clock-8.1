<UserControl x:Class="Live_Clock_Tile.Core.WideClocks.GlowWide"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    d:DesignHeight="336" d:DesignWidth="691">

    <Grid x:Name="LayoutRoot">
        <TextBlock x:Name="hour" HorizontalAlignment="Left" TextWrapping="Wrap" Text="12:36" VerticalAlignment="Top" FontSize="260" Foreground="#FFFDFDFD" Width="691" Height="326" FontFamily="Segoe WP Light" TextAlignment="Center" Margin="0,-21,0,0"/>

    </Grid>
</UserControl>
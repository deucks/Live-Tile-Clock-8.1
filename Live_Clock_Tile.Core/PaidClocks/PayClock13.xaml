<UserControl x:Class="Live_Clock_Tile.Core.PaidClocks.PayClock13"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    d:DesignHeight="336" d:DesignWidth="336">

    <Grid x:Name="LayoutRoot">
        <TextBlock x:Name="hour" HorizontalAlignment="Left" FontFamily="../Fonts/digital-7.ttf#digital-7" TextWrapping="Wrap" Text="09:20" VerticalAlignment="Top" FontSize="160" Margin="0,95,0,0" TextAlignment="Center" Width="336" Foreground="#DEFDFDFD" Height="168"/>
        <TextBlock x:Name="ampm" HorizontalAlignment="Left" FontFamily="../Fonts/digital-7.ttf#digital-7" TextWrapping="Wrap" Text="PM" VerticalAlignment="Top" FontSize="50" Margin="0,263,0,0" TextAlignment="Center" Width="336" Foreground="#DEFDFDFD"/>
        <TextBlock x:Name="month" HorizontalAlignment="Left" FontFamily="../Fonts/digital-7.ttf#digital-7" TextWrapping="Wrap" Text="Wed. 18" VerticalAlignment="Top" FontSize="50" Margin="0,41,0,0" TextAlignment="Center" Width="336" Foreground="#DEFDFDFD"/>

    </Grid>
</UserControl>
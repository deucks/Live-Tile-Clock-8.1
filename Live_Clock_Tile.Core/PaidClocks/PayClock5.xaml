<UserControl x:Class="Live_Clock_Tile.PaidClocks.PayClock5"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    d:DesignHeight="336" d:DesignWidth="336">

    <Grid x:Name="LayoutRoot">
        <Ellipse HorizontalAlignment="Left" Height="316" Margin="10,10,0,0" Stroke="White" StrokeThickness="5" VerticalAlignment="Top" Width="316"/>
        <TextBlock x:Name="hour" HorizontalAlignment="Left" Margin="0,92,0,0" TextWrapping="Wrap" Text="05" VerticalAlignment="Top" FontSize="150" Foreground="#FFFDFDFD" Width="336" Height="151" FontFamily="MS UI Gothic" TextAlignment="Center" FontWeight="Bold" Opacity="0.7"/>
        <Rectangle x:Name="minute" HorizontalAlignment="Left" Height="53" Stroke="White" StrokeThickness="3" VerticalAlignment="Top" Width="5" Margin="168,24,0,0" RenderTransformOrigin="0,2.711">
        </Rectangle>

    </Grid>
</UserControl>
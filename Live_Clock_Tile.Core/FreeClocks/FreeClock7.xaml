<UserControl x:Class="Live_Clock_Tile.FreeClocks.FreeClock7"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    d:DesignHeight="336" d:DesignWidth="336">

    <Grid x:Name="LayoutRoot">
        <TextBlock x:Name="time" HorizontalAlignment="Left" Margin="0,167,0,0" TextWrapping="Wrap" Text="08:08" VerticalAlignment="Top" FontSize="140" Foreground="#FFFDFDFD" Width="336" Height="169" FontFamily="Segoe WP" TextAlignment="Right"/>
    </Grid>
</UserControl>
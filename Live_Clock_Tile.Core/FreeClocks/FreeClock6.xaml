<UserControl x:Class="Live_Clock_Tile.FreeClocks.FreeClock6"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    d:DesignHeight="336" d:DesignWidth="336">

    <Grid x:Name="LayoutRoot">
        <Ellipse Fill="Transparent" HorizontalAlignment="Left" Height="316" Stroke="White" StrokeThickness="15" VerticalAlignment="Top" Width="316" Margin="10,10,0,0" Opacity="0.7"/>
        <Ellipse Fill="Transparent" HorizontalAlignment="Left" Height="284" Stroke="White" StrokeThickness="10" VerticalAlignment="Top" Width="284" Margin="26,26,0,0"/>
        <Grid x:Name="minute" HorizontalAlignment="Left" Height="85" VerticalAlignment="Top" Width="4" Background="White" Margin="166,83,0,0" RenderTransformOrigin="0.531,1.002">
        </Grid>
        <Grid x:Name="hour" HorizontalAlignment="Left" Height="60" VerticalAlignment="Top" Width="4" Background="White" Margin="166,108,0,0" RenderTransformOrigin="0.531,1.002"/>

    </Grid>
</UserControl>
<UserControl x:Class="Live_Clock_Tile.Core.FreeClocks.FreeClock13"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    d:DesignHeight="336" d:DesignWidth="336">

    <Grid x:Name="LayoutRoot">
        <TextBlock x:Name="hour" HorizontalAlignment="Left" Margin="0,57,0,0" TextWrapping="Wrap" Text="11" VerticalAlignment="Top" Foreground="#DEFDFDFD" FontFamily="Khmer UI" FontSize="120" Width="163" FontWeight="Bold" TextAlignment="Right"/>
        <TextBlock x:Name="minute" HorizontalAlignment="Left" Margin="163,37,0,0" TextWrapping="Wrap" Text=":37" VerticalAlignment="Top" Foreground="#DEFDFDFD" FontFamily="Segoe WP SemiLight" FontSize="120" Width="173" Height="172"/>
        <TextBlock x:Name="month" HorizontalAlignment="Left" Margin="0,193,0,0" TextWrapping="Wrap" Text="Wednesday, September 18" VerticalAlignment="Top" Foreground="#DEFDFDFD" FontFamily="Segoe WP SemiLight" FontSize="40" Width="336" Height="133" TextAlignment="Center"/>
    </Grid>
</UserControl>